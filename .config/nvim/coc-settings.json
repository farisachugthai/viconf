{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "css",
    "vim",
    "bash",
    "powershell",
    "python"
  ],
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.messageLevel": "error",
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.snippetStatusText": "",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.source.around.shortcut": "Coc-Around:",
  "coc.source.buffer.firstMatch": false,
  "coc.source.buffer.shortcut": "CocBuffer: ",
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.priority": 80,
  "coc.source.file.shortcut": "CocFiles: ",
  "coc.source.issues.shortcut": "Coc-Issues:",
  "coc.source.omni.priority": 99,
  "coc.source.omni.shortcut": "Coc-Omni:",
  "coc.source.omni.triggerCharacters": [
    "Tab"
  ],
  "coc.source.ultisnips.filetypes": [
    "python",
    "html",
    "css",
    "sh",
    "powershell",
    "typescript"
  ],
  "coc.source.ultisnips.shortcut": "Coc-UltiSnips:",
  "codeLens.enable": true,
  "codeLens.separator": "‣",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.filetypeMap": {"default": "bufferType"},
  "diagnostic.joinMessageLines": true,
  "diagnostic.level": "warning",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshAfterSave": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextPrefix": "Coc-Diagnostic: ",
  "diagnostic.virtualTextLines": 3,
  "git.addGlameToBufferVar": true,
  "git.addGlameToVirtualText": true,
  "git.branchCharacter": "",
  "git.enableGlobalStatus": true,
  "git.enableGutters": true,
  "git.stagedDecorator": " - Staged",
  "git.untrackedDecorator": "Untracked:",
  "git.conflictedDecorator": "Merge conflict",
  "git.virtualTextPrefix": "Coc-Git:",
  "languageserver": {
    "python": {
      "args": [
        "-vv",
        "--log-file",
        "~/.local/share/nvim/site/lsp_python.log"
      ],
      "command": "python",
      "filetypes": [
        "python"
      ],
      "shell": true,
      "settings": {
        "pyls": {
          "commandPath": "python -m pyls",
          "configurationSources": [
            "flake8"
          ],
          "enable": true,
          "trace": {
            "server": "verbose"
          },
          "plugins": {
            "flake8": {
              "enabled": true
            },
            "jedi_completion": {
              "enabled": true
            },
            "jedi_hover": {
              "enabled": true
            },
            "jedi_references": {
              "enabled": true
            },
            "jedi_signature_help": {
              "enabled": true
            },
            "jedi_symbols": {
              "all_scopes": true,
              "enabled": true
            },
            "mccabe": {
              "enabled": true,
              "threshold": 15
            },
            "preload": {
              "enabled": true
            },
            "pycodestyle": {
              "enabled": false
            },
            "pydocstyle": {
              "enabled": true,
              "match": "(?!test_).*\\.py",
              "matchDir": "[^\\.].*"
            },
            "pyflakes": {
              "enabled": true
            },
            "rope_completion": {
              "enabled": true
            },
            "yapf": {
              "enabled": true
            }
          },
          "trace": {
            "server": "verbose"
          }
        }
      }
    }
  },
  "list.extendedSearchMode": true,
  "list.insertMappings": {
    "<A-p>": "action:preview",
    "<C-V>": "eval:\"+",
    "<C-d>": "prompt:deleteforward",
    "<C-h>": "do:help",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-r>": "do:refresh",
    "<C-t>": "action:tabe",
    "<C-v>": "eval:@@",
    "<C-x>": "action:split",
    "<CR>": "do:defaultaction"
  },
  "list.limitLines": 500,
  "list.normalMappings": {
    " ": "action : toggle",
    "<C-r>": "do:refresh",
    "<C-t>": "action : tabe",
    "<C-x>": "action : split",
    "<CR>" : "do : defaultaction",
    "C": "action:quickfix",
    "D": "action : delete",
    "E": "action : edit",
    "H": "action : help",
    "L": "action : load",
    "O": "action:open",
    "P": "action : preview",
    "c": "action:quickfix",
    "d": "action : delete",
    "e": "action : edit",
    "h": "action : help",
    "l": "action : load",
    "o": "action:open",
    "p": "action : preview"
  },
  "list.previewHighlightGroup": "IdentifierBold",
  "list.source.actions.defaultArgs": [
    "-quickfix"
  ],
  "list.source.actions.defaultOptions": [
    "--normal",
    "--top",
    "--auto-preview"
  ],
  "list.source.commands.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.diagnostics.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.folders.defaultOptions": ["--top", "--auto-preview", "--normal", "--interactive"],
  "list.source.files.command": "rg",
  "list.source.files.args": [
    "--hidden",
    "--smart-case",
    "--no-column",
    "--no-line-number",
    "--no-messages",
    "--crlf",
    "--color=never",
    "--files"
  ],
  "list.source.files.defaultOptions": [
    "--number-select",
    "--auto-preview",
    "--top"
  ],
  "list.source.files.excludePatterns": [
    "**/node_modules/*",
    "**/.git/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.folders.defaultOptions": [
    "--top",
    "--auto-preview",
    "--normal",
    "--interactive"
  ],
  "list.source.grep.args": [
    "--hidden",
    "--smart-case",
    "--no-column",
    "--no-line-number",
    "--no-messages",
    "--crlf",
    "--color=never",
    "--files"
  ],
  "list.source.grep.defaultArgs": [
    "-smartcase",
    "-regex",
    "-workspace"
  ],
  "list.source.grep.defaultOptions": [
    "--auto-preview",
    "--normal",
    "--interactive"
  ],
  "list.source.grep.useLiteral": false,
  "list.source.helptags.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.lines.defaultOptions": [
    "--top",
    "--interactive",
    "--normal",
    "--auto-preview"
  ],
  "list.source.links.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.lists.defaultOptions": [
    "--normal",
    "--top"
  ],
  "list.source.location.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.mru.defaultOptions": [
    "--auto-preview",
    "--normal"
  ],
  "list.source.mru.maxLength": 100,
  "list.source.mru.excludePatterns": [
    "**/.git/*",
    "**/node_modules/*",
    "/tmp/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.output.defaultOptions": [
    "--normal",
    "--top"
  ],
  "list.source.services.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.sources.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.symbols.defaultOptions": [
    "--interactive",
    "--number-select"
  ],
  "list.source.tags.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.words.defaultArgs": [
    "-word"
  ],
  "list.source.words.defaultOptions": [
    "--top",
    "--interactive",
    "--normal",
    "--auto-preview"
  ],
  "lists.disabledLists": [
    "buffers",
    "bcommits",
    "commits",
    "colors",
    "filetypes",
    "marks",
    "snippets",
    "vimcommands",
    "windows"
  ],
  "powershell.codeFolding.showLastLine": false,
  "powershell.codeFormatting.openBraceOnSameLine": true,
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.debugging.createTemporaryIntegratedConsole": false,
  "powershell.developer.editorServicesLogLevel": "Error",
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.powerShellExePath": "C:\\pwsh\\7-preview\\pwsh.exe",
  "python.analysis.diagnosticEnabled": true,
  "python.analysis.logLevel": "Warning",
  "python.analysis.openFilesOnly": false,
  "python.autoComplete.addBrackets": true,
  "python.jediMemoryLimit": -1,
  "python.jediShortcut": "Jedi",
  "python.linting.pylintEnabled": false,
  "python.terminal.executeInFileDir": true,
  "python.terminal.launchArgs": [
    "-m",
    "IPython"
  ],
  "python.trace.server": "messages",
  "python.venvFolders": [
    "~/virtualenvs",
    "~/.local/share/virtualenvs/"
  ],
  "refactor.afterContext": 0,
  "refactor.beforeContext": 0,
  "refactor.openCommand": "vsplit",
  "signature.hideOnTextChange": true,
  "signature.preferShownAbove": false,
  "signature.target": "float",
  "signature.triggerSignatureWait": 300,
  "snippets.extends": {
    "python": [
      "rst"
    ]
  },
  "snippets.shortcut": "Coc-Snippets",
  "snippets.snipmate.author": "Faris A. Chugthai",
  "snippets.trace": "verbose",
  "snippets.triggerCharacters": [
    "Tab",
    "C-j"
  ],
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips",
    "~/AppData/Local/nvim/UltiSnips"
  ],
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.completionItemKindLabels": {
    "class": "",
    "color": "",
    "constant": "",
    "default": "",
    "enum": "",
    "enumMember": "",
    "event": "鬒",
    "field": "料",
    "file": "",
    "folder": "",
    "function": "ƒ",
    "interface": "",
    "keyword": "",
    "method": "",
    "module": "",
    "operator": "Ψ",
    "property": "",
    "reference": "渚",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.detailField": "preview",
  "suggest.disableMenu": true,
  "suggest.disableMenuShortcut": true,
  "suggest.enablePreview": true,
  "suggest.enablePreselect": false,
  "suggest.fixInsertedWord": true,
  "suggest.floatEnable": true,
  "suggest.keepCompleteopt": true,
  "suggest.lowPrioritySourceLimit": 50,
  "suggest.highPrioritySourceLimit": 99,
  "suggest.maxCompleteItemCount": 100,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.snippetIndicator": "[Coc-Snippets:]",
  "suggest.timeout": 800,
  "suggest.triggerAfterInsertEnter": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.locale": "utf8",
  "tsserver.log": "normal",
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "vimlsp.indexes.runtimepath": true,
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.suggest.fromVimruntime": true
}
