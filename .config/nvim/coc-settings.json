{
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.completionItemKindLabels": {
    "keyword"       : "\uf1de",
    "variable"      : "\ue79b",
    "value"         : "\uf89f",
    "operator"      : "\u03a8",
    "function"      : "\u0192",
    "reference"     : "\ufa46",
    "constant"      : "\uf8fe",
    "method"        : "\uf09a",
    "struct"        : "\ufb44",
    "class"         : "\uf0e8",
    "interface"     : "\uf417",
    "text"          : "\ue612",
    "enum"          : "\uf435",
    "enumMember"    : "\uf02b",
    "module"        : "\uf40d",
    "color"         : "\ue22b",
    "property"      : "\ue624",
    "field"         : "\uf9be",
    "unit"          : "\uf475",
    "event"         : "\ufacd",
    "file"          : "\uf723",
    "folder"        : "\uf114",
    "snippet"       : "\ue60b",
    "typeParameter" : "\uf728",
    "default"       : "\uf29c"
  },
  "suggest.detailField": "preview",
  "suggest.enablePreview": true,
  "suggest.fixInsertedWord": true,
  "suggest.floatEnable": true,
  "suggest.keepCompleteopt": true,
  // why is this producing an error?
  // "suggest.LowPrioritySourceLimit": 0,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.snippetIndicator": "[Coc:]",
  "suggest.timeout": 800,
  "suggest.triggerAfterInsertEnter": true,

  "python.analysis.diagnosticEnabled": true,
  "python.analysis.logLevel": "Warning",
  "python.analysis.openFilesOnly": false,
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": true,
  "python.jediMemoryLimit": -1,
  "python.jediShortcut": "Jedi",
  "python.linting.pylintEnabled": false,
  "python.terminal.executeInFileDir": true,
  "python.trace.server": "messages",
  "python.venvFolders": [
    "~/virtualenvs",
    "~/.local/share/virtualenvs/",
    "~/.virtualenvs",
    "~/miniconda3/envs"
  ],
  "python.terminal.launchArgs": ["-c", "IPython"],
  "languageserver": {
    "python": {
      "command": "python",
      "args": [
        "-vv",
        "--log-file",
        "~/.local/share/nvim/site/lsp_python.log"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "python"
      ],
      "settings": {
        "pyls": {
          "enable": true,
          "trace": {
            "server": "verbose"
          },
          "commandPath": "python -m pyls",
          "configurationSources": [
            "flake8"
          ],
          "plugins": {
            "jedi_completion": {
              "enabled": true
            },
            "jedi_hover": {
              "enabled": true
            },
            "jedi_references": {
              "enabled": true
            },
            "jedi_signature_help": {
              "enabled": true
            },
            "jedi_symbols": {
              "enabled": true,
              "all_scopes": true
            },
            "mccabe": {
              "enabled": true,
              "threshold": 15
            },
            "preload": {
              "enabled": true
            },
            "pycodestyle": {
              "enabled": false
            },
            "pydocstyle": {
              "enabled": true,
              "match": "(?!test_).*\\.py",
              "matchDir": "[^\\.].*"
            },
            "pyflakes": {
              "enabled": true
            },
            "flake8": {
              "enabled": true
            },
            "rope_completion": {
              "enabled": true
            },
            "yapf": {
              "enabled": true
            }
          }
        }
      }
    }
  },

  "list.insertMappings": {
    "<C-r>": "do:refresh",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-t>": "action:tabe",
    "<C-x>": "action:split",
    "<CR>" : "do:defaultaction",
    "<A-p>": "action:preview",
    "<C-h>": "do:help",
	"<C-v>": "eval:@@",
	"<C-V>": "eval:\"+",
    "<C-d>": "prompt:deleteforward"
  },
  "list.normalMappings": {
    "<C-r>" : "do:refresh",
    "d"     : "action : delete",
    "D"     : "action : delete",
    "e"     : "action : edit",
    "E"     : "action : edit",
    "s"     : "action : split",
    "S"     : "action : split",
    "T"     : "action : tabe",
    "P"     : "action : preview",
    "p"     : "action : preview",
    "l"     : "action : load",
    "L"     : "action : load",
    "h"     : "action : help",
    "H"     : "action : help",
    " "     : "action : toggle",
    "C"     : "action : quickfix",
    "c"     : "action : quickfix",
    "o"     : "action : open",
    "O"     : "action : open",
    "<CR>"  : "action : select"
  },

  "list.source.actions.defaultArgs": ["-quickfix"],
  "list.source.actions.defaultOptions": [
    "--interactive", "--top", "--normal", "--number-select"
  ],
  "list.source.branches.defaultOptions": [
    "--interactive", "--top", "--normal", "--number-select"
  ],

  "list.source.diagnostics.defaultOptions": ["--top", "--normal"],
  "list.source.extensions.defaultOptions": ["--top", "--normal"],

  "list.source.files.command": "rg",
  "list.source.files.args": [
    "--hidden", "--smart-case", "--no-column", "--no-line-number",
    "--no-messages", "--crlf", "--color=never", "--files"
  ],
  "list.source.files.excludePatterns": [
    "**/node_modules/*",
    "**/.git/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.files.defaultOptions": [
    "--number-select", "--auto-preview", "--top", "--normal"
  ],
  "list.source.folders.defaultOptions": [
    "--top", "--auto-preview", "--normal", "--interactive"
  ],

  "list.source.grep.useLiteral": false,
  "list.source.grep.defaultArgs": ["-smartcase", "-regex", "-workspace"],
  "list.source.grep.defaultOptions": [
    "--auto-preview", "--normal", "--interactive", "--top"
  ],
  "list.source.grep.args": [
    "--hidden", "--smart-case", "--no-column", "--no-line-number",
    "--no-messages", "--crlf", "--color=never", "--files"
   ],

  "list.source.helptags.defaultOptions": [
    "--top", "--auto-preview", "--normal", "--interactive"
  ],
  "list.source.lines.defaultOptions": [
    "--top", "--interactive", "--normal", "--auto-preview"
  ],
  "list.source.links.defaultOptions": ["--top", "--normal"],

  "list.source.lists.defaultOptions": ["--normal", "--top"],

  "list.source.mru.maxLength": 100,
  "list.source.mru.defaultOptions": [
    "--auto-preview", "--normal", "--top"
  ],
  "list.source.mru.ignoreGitIgnore": true,
  "list.source.mru.excludePatterns": [
    "**/.git/*",
    "**/node_modules/*",
    "/tmp/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.output.defaultOptions": [
    "--auto-preview", "--interactive", "--no-sort", "--top", "--normal" 
  ],

  "list.source.sessions.defaultOptions": [
    "--normal", "--top", "--number-select"
  ],
  "list.source.services.defaultOptions": ["--normal", "--top"],
  "list.source.symbols.defaultOptions": [
    "--interactive", "--number-select", "--normal", "--top"
  ],

  // as weird as it looks don't use an array
  "list.source.tags.command":
    "ctags -R --options=~/projects/dynamic_ipython/tools/ctagsOptions.cnf .",
  "list.source.tags.defaultOptions": ["--top", "--normal"],

  "list.source.words.defaultArgs": ["--word"],
  "list.source.words.defaultOptions": [
    "--top", "--interactive", "--normal", "--auto-preview" 
  ],

  // Disable them because FZF or Fugitive already does it
  "lists.disabledLists": ["colors", "filetypes", "snippets", "bcommits",
  "buffers", "filetypes", "maps", "vimcommands"],
  "list.extendedSearchMode": true,
  "list.limitLines": 500,
  "list.previewHighlightGroup": "IdentifierBold",

  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript", "html", "css", "vim", "bash", "sh", "powershell", "python"
  ],
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.messageLevel": "error",
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.useQuickfixForLocations": true,

  "coc.source.buffer.firstMatch": false, 
  "coc.source.buffer.shortcut": "CocBuffer: ",
  "coc.source.file.shortcut": "CocFiles: ",
  "coc.source.file.priority": 80,
  "coc.source.file.ignoreHidden": false,
  "coc.source.omni.priority": 99,

  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.level": "warning",
  "diagnostic.joinMessageLines": true,
  "diagnostic.virtualText": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "signature.hideOnTextChange": true,
  "signature.floatMaxWidth": 20,
  "signature.preferShownAbove": false,
  "signature.triggerSignatureWait": 300,
  "vimlsp.suggest.fromVimruntime": true,
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.indexes.runtimepath": true,
  "yaml.format.enable": true,
  "git.addGlameToVirtualText": true,
  "git.addGlameToBufferVar": true,
  "git.branchCharacter": "",
  "git.changedDecorator": "⎇",
  "git.enableGlobalStatus": true,
  "git.untrackedDecorator": "Untracked:",
  "git.virtualTextPrefix": "Coc-Git:",
  "codeLens.separator": "‣",
  "codeLens.enable": true,
  "session.directory": "~/.config/nvim/session",
  "powershell.powerShellExePath": "C:\\pwsh\\7-preview\\pwsh.exe",
  "powershell.codeFolding.showLastLine": false,
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.debugging.createTemporaryIntegratedConsole": false,
  "powershell.codeFormatting.openBraceOnSameLine": true,
  "powershell.developer.editorServicesLogLevel": "Error",
  "powershell.integratedConsole.focusConsoleOnExecute": false,
  "powershell.codeFormatting.useCorrectCasing": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "refactor.openCommand": "vsplit",
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips",
    "~/AppData/Local/nvim/UltiSnips"
  ],
  "snippets.extends": {"python": ["rst"]},
  "snippets.snipmate.author": "Faris A. Chugthai",
  "snippets.trace": "verbose",
  "snippets.triggerCharacters": ["Tab", "C-j"]
}
