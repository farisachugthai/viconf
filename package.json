{
  "name": "coc-extensions",
  "license": "MIT",
  "private": true,
  "keywords": [
    "coc.nvim",
    "nvim",
    "coc"
  ],
  "author": "Faris Chugthai",
  "bugs": {
    "url": "https://github.com/farisachugthai/viconf/issues"
  },
  "activationEvents": [
    "onLanguage:vim"
  ],
  "engines": {
    "coc": ">= 0.0.77"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "coc-css": ">=1.2.2",
    "coc-dictionary": "^1.2.2",
    "coc-eslint": ">=1.2.6",
    "coc-git": ">=1.7.9",
    "coc-highlight": ">=1.2.5",
    "coc-html": ">=1.2.4",
    "coc-json": ">=1.2.5",
    "coc-lists": ">=1.3.7",
    "coc-prettier": "^1.1.12",
    "coc-python": ">=1.2.9",
    "coc-sh": ">=0.4.0",
    "coc-snippets": ">=2.1.23",
    "coc-tslint-plugin": ">=1.1",
    "coc-tsserver": ">=1.4.12",
    "coc-vimlsp": ">=0.5.1",
    "coc-word": ">=1.2.2",
    "coc-yaml": ">=1.0.4",
    "coc-yank": ">=1.1.4",
    "vim-language-server": "^1.5.2",
    "webpack-cli": "^3.3.11",
    "yaml-language-server": "^0.6.1"
  },
  "directories": {
    "lib": "rplugin/node"
  },
  "main": "rplugin/node/coc_tag.js",
  "peerDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.4",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  },
  "readme": "README.rst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farisachugthai/viconf.git"
  },
  "scripts": {
    "build": "yarn run webpack -dw rplugin/node/coc_tag.js",
    "clean": "yarn autoclean --force",
    "fix": "yarn run prettier --write rplugin/node",
    "lint": "yarn run tslint --fix **.ts",
    "pretest": "echo 'Running tests'",
    "start": "yarn run vim-language-server --stdio",
    "tagfunc": "node ./rplugin/node/coc_tag.js",
    "tsserver": "node ./node_modules/coc-tsserver/bin/tsserverForkStart.js",
    "watch": "yarn run tsc -w rplugin/node"
  },
  "version": "1.0.0",
  "dependencies": {
    "webpack": "^4.42.1"
  }
}
