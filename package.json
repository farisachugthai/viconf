{
  "name": "viconf",
  "license": "MIT",
  "description": "Mostly here for Coc extensions",
  "readme": "README.rst",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farisachugthai/viconf.git"
  },
  "scripts": {
    "lint": "yarn run tslint -p .",
    "fix": "yarn run prettier rplugin/node",
    "tagfunc": "./rplugin/node/coc_tag.js",
    "watch": "yarn run tsc -w -p rplugin/node",
    "build": "yarn run rimraf ./out && yarn run webpack",
    "start": "yarn run vim-language-server --stdio"
  },
  "main": "rplugin/node/coc_tag.js",
  "directories": {
    "lib": "rplugin/node"
  },
  "author": "Faris Chugthai",
  "bugs": {
    "url": "https://github.com/farisachugthai/viconf/issues"
  },
  "keywords": [
    "coc.nvim",
    "nvim",
    "coc"
  ],
  "engines": {
    "coc": "^0.0.71"
  },
  "activationEvents": [
    "onLanguage:vim",
    "onLanguage:json",
    "onLanguage:python"
  ],
  "version": "1.0.0",
  "devDependencies": {
    "@types/vscode": "^1.41.0",
    "coc-dictionary": "^1.2.2",
    "coc-html": ">=1.2.1",
    "coc-json": ">=1.2.4",
    "coc-lists": ">=1.3.7",
    "coc-python": "^1.2.7",
    "coc-snippets": ">=2.1.18",
    "coc-tsserver": ">=1.4.9",
    "coc-vimlsp": ">=0.4.5",
    "coc-yank": ">=1.1.3"
  }
}
