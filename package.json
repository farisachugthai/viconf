{
  "activationEvents": [
    "*"
  ],
  "author": "Faris Chugthai",
  "bugs": {
    "url": "https://github.com/farisachugthai/viconf/issues"
  },
  "contributes": {
    "commands": [
      {
        "command": "coc-ext.version",
        "title": "Print extension version"
      },
      {
        "command": "coc-ext.update",
        "title": "Update coc-ext"
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "~/.config/coc/ultisnips/python.json"
      },
      {
        "language": "typescript",
        "path": "~/.config/coc/ultisnips/typescript.json"
      }
    ],
    "configuration": {
      "title": "coc-extensions",
      "type": "object",
      "properties": {
        "coc-extensions.enable": {
          "type": "boolean",
          "defaul": true
        },
        "coc-extension.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of extensions."
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "coc-css": ">=1.2.3",
    "coc-dictionary": "^1.2.2",
    "coc-eslint": ">=1.2.6",
    "coc-git": ">=1.7.13",
    "coc-html": ">=1.2.4",
    "coc-json": ">=1.2.6",
    "coc-lists": ">=1.3.8",
    "coc-python": ">=1.2.12",
    "coc-snippets": ">=2.1.26",
    "coc-tslint-plugin": ">=1.1.1",
    "coc-tsserver": ">=1.5.1",
    "coc-vimlsp": "^0.8.0",
    "eslint": "^7.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "directories": {
    "lib": "rplugin/node"
  },
  "engines": {
    "coc": ">= 0.0.77"
  },
  "keywords": [
    "coc.nvim",
    "nvim",
    "coc"
  ],
  "license": "MIT",
  "name": "coc-extensions",
  "peerDependencies": {
    "coc.nvim": "^0.0.77"
  },
  "private": true,
  "readme": "README.rst",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farisachugthai/viconf.git"
  },
  "scripts": {
    "build": "yarn run webpack -dw rplugin/node/coc_tag.ts",
    "build:watch": "tsc --watch -p tsconfig.json",
    "clean": "yarn autoclean --force",
    "coc-tsserver": "node ./node_modules/coc-tsserver/bin/tsserverForkStart.js",
    "fix": "yarn run prettier --write rplugin/node",
    "lint": "yarn run tslint --fix **.ts",
    "pretest": "echo 'Running tests'",
    "start": "yarn run neovim-node-host",
    "tagfunc": "node ./rplugin/node/coc_tag.ts",
    "tsserver": "node ./node_modules/typescript/bin/tsserver",
    "vimlsp": "yarn run vim-language-server --stdio",
    "watch": "yarn run tsc -w rplugin/node"
  },
  "version": "1.0.0",
  "dependencies": {
    "neovim": "^4.8.0"
  }
}
