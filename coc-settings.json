    "color": "",
    "constant": "",
    "default": "",
    "enum": "Ψ",
    "enumMember": "",
    "event": "鬒",
    "field": "料",
    "file": "",
    "folder": "",
    "function": "ƒ",
    "interface": "",
    "keyword": "",
    "method": "",
    "module": "",
    "operator": "",
    "property": "",
    "reference": "渚",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.enablePreview": true,
  "suggest.fixInsertedWord": true,
  "suggest.floatEnable": true,
  "suggest.keepCompleteopt": true,
  "suggest.maxCompleteItemCount": 100,
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.removeDuplicateItems": true,
  "suggest.timeout": 800,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.triggerCompletionWait": 50,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.locale": "utf8",
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.referencesCodeLens.enable": true,
  "typescript.showUnused": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeFunctionCalls": true,
  "vimlsp.diagnostic.enable": true,
  "vimlsp.indexes.count": 3,
  "vimlsp.indexes.runtimepath": true,
  "vimlsp.suggest.fromRuntimepath": true,
  "vimlsp.suggest.fromVimruntime": true,
