    "**/site-packages/**",
    "node_modules",
    "build",
    "dist",
    ".tox",
    ".venv"
  ],
  "refactor.openCommand": "vsplit",
  "session.restartOnSessionLoad": true,
  "signature.hideOnTextChange": true,
  "signature.maxWindowHeight": 14,
  "signature.preferShownAbove": false,
  "signature.triggerSignatureWait": 300,
  "snippets.editSnippetsCommand": "tabe",
