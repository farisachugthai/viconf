{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "css",
    "vim",
    "bash",
    "powershell",
    "python"
  ],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.previewAutoClose": true,
  "coc.source.around.firstMatch": false,
  "coc.source.around.shortcut": "Coc-Around:",
  "coc.source.buffer.firstMatch": false,
  "coc.source.buffer.ignoreGitignore": false,
  "coc.source.buffer.shortcut": "Coc-Buffer:",
  "coc.source.dictionary.filetypes": [
    "rst",
    "text",
    "json",
    "help",
    "html"
  ],
  "coc.source.dictionary.priority": 40,
  "coc.source.dictionary.shortcut": "Coc-Dict:",
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.priority": 20,
  "coc.source.file.shortcut": "Coc-Files: ",
  "coc.source.file.trimSameExts": [
    ".ts",
    ".js"
  ],
  "coc.source.word.filetypes": [
    "python",
    "javascript",
    "rst",
    "md",
    "txt",
    "help",
    "html"
  ],
  "coc.source.word.priority": 30,
  "coc.source.word.shortcut": "Coc-Word: ",
  "codeLens.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.filetypeMap": {
    "default": "bufferType"
  },
  "diagnostic.joinMessageLines": true,
  "diagnostic.level": "warning",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "javascript.referencesCodeLens.enable": true,
  "javascript.showUnused": true,
  "json.format.enable": true,
  "languageserver": {
    "vimlsp": {
      "args": [
        "--stdio"
      ],
      "command": "vim-language-server",
      "filetypes": [
        "vim"
      ],
      "initializationOptions": {
        "diagnostic": {
          "enable": true
        },
        "indexes": {
          "count": 3,
          "gap": 100,
          "runtimepath": true,
          "workDirPatterns": [
            ".git",
            "autoload",
            "plugin"
          ]
        },
        "iskeyword": "@,48-57,_,192-255,-#",
        "runtimepath": false,
        "suggest": {
          "fromRuntimepath": false,
          "fromVimruntime": true
        },
        "vimruntime": "$VIMRUNTIME"
      }
    }
  },
  "list.autoResize": true,
  "list.extendedSearchMode": true,
  "list.indicator": "CocList: >>> $: ",
  "list.interactiveDebounceTime": 200,
  "list.limitLines": 500,
  "list.maxHeight": 20,
  "list.maxPreviewHeight": 25,
  "list.nextKeymap": "<C-n>",
  "list.previewHighlightGroup": "IdentifierBold",
  "list.previousKeymap": "<C-p>",
  "list.selectedSignText": "****",
  "list.signOffset": 20,
  "list.source.actions.defaultArgs": [
    "quickfix"
  ],
  "list.source.actions.defaultOptions": [
    "--normal",
    "--top"
  ],
  "list.source.cmdhistory.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.commands.defaultOptions": [
