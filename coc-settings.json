{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "css",
    "vim",
    "bash",
    "powershell",
    "python"
  ],
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.previewAutoClose": true,
  "coc.source.around.firstMatch": false,
  "coc.source.around.priority": 90,
  "coc.source.around.shortcut": "Coc-Around:",
  "coc.source.buffer.firstMatch": false,
  "coc.source.buffer.ignoreGitignore": false,
  "coc.source.buffer.priority": 70,
  "coc.source.buffer.shortcut": "Coc-Buffer:",
  "coc.source.dictionary.filetypes": [
    "rst",
    "text",
    "json",
    "help",
    "html"
  ],
  "coc.source.dictionary.priority": 40,
  "coc.source.dictionary.shortcut": "Coc-Dict:",
  "coc.source.file.ignoreHidden": false,
  "coc.source.file.priority": 20,
  "coc.source.file.shortcut": "Coc-Files: ",
  "coc.source.file.trimSameExts": [
    ".ts",
    ".js"
  ],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.filetypeMap": {
    "default": "bufferType"
  },
  "javascript.referencesCodeLens.enable": true,
  "javascript.showUnused": true,
  "list.indicator": "CocList: >>> $: ",
  "list.nextKeymap": "<C-n>",
  "list.previewHighlightGroup": "IdentifierBold",
  "list.previousKeymap": "<C-p>",
  "list.selectedSignText": "****",
  "list.signOffset": 20,
  "list.interactiveDebounceTime": 400,
  "list.source.actions.defaultArgs": [
    "quickfix"
  ],
  "list.source.commands.defaultOptions": [
    "--top",
    "--no-sort",
    "--ignore-case"
  ],
  "list.source.diagnostics.defaultArgs": [
    "preview"
  ],
  "list.source.diagnostics.defaultOptions": [
    "--auto-preview",
    "--no-sort"
  ],
  "list.source.files.args": [
    "--hidden",
    "--smart-case",
    "--no-column",
    "--no-line-number",
    "--no-messages",
    "--crlf",
    "--color=never",
    "--files"
  ],
  "list.source.files.command": "rg",
  "list.source.files.defaultOptions": [
    "--number-select",
    "--auto-preview",
    "--top"
  ],
  "list.source.files.excludePatterns": [
    "**/node_modules/*",
    "**/.git/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.folders.defaultArgs": [
    "edit"
  ],
  "list.source.folders.defaultOptions": [
    "--top",
    "--input",
    "--tab",
    "--number-select"
  ],
  "list.source.grep.args": [
    "--hidden",
    "--smart-case",
    "--no-column",
    "--no-line-number",
    "--no-messages",
    "--color=never",
    "--files"
  ],
  "list.source.grep.command": "rg",
  "list.source.grep.defaultArgs": [
    "-smartcase",
    "-regex",
    "-workspace"
  ],
  "list.source.grep.defaultOptions": [
    "--auto-preview",
    "--normal",
    "--interactive"
  ],
  "list.source.grep.excludePatterns": [
    "**/node_modules/*",
    "**/.git/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.grep.maxColumns": 2000,
  "list.source.grep.useLiteral": false,
  "list.source.helptags.defaultOptions": [
    "--top",
    "--auto-preview",
    "--tab",
    "--normal"
  ],
  "list.source.lines.defaultOptions": [
    "--top",
    "--interactive",
    "--normal",
    "--auto-preview"
  ],
  "list.source.mru.defaultOptions": [
    "--auto-preview",
    "--top",
    "--normal"
  ],
  "list.source.mru.excludePatterns": [
    "**/.git/*",
    "**/node_modules/*",
    "/tmp/*",
    "**/__pycache__/**",
    "**/*.pyc"
  ],
  "list.source.mru.ignoreGitIgnore": true,
  "list.source.mru.maxLength": 100,
  "list.source.outline.ctagsFilestypes": [
    "python",
    "javascript",
    "rst",
    "markdown",
    "text"
  ],
  "list.source.outline.defaultOptions": [
    "--top",
    "--input",
    "--normal"
  ],
  "list.source.outline.defaultArgs": [
    "preview"
  ],
  "list.source.output.defaultOptions": [
    "--input",
    "--number-select",
    "--no-sort",
    "--top"
  ],
  "list.source.searchhistory.defaultOptions": [
    "--top",
    "--tab",
    "--regex"
  ],
  "list.source.snippets.defaultOptions": [
    "--auto-preview",
    "--top",
    "--ignore-case"
  ],
  "list.source.sources.defaultArgs": [
    "-refresh"
  ],
  "list.source.symbols.defaultOptions": [
    "--top",
    "--normal",
    "--interactive",
    "--number-select"
  ],
  "list.source.words.defaultArgs": [
    "-word"
  ],
  "list.source.words.defaultOptions": [
    "--top",
    "--interactive",
    "--normal",
    "--auto-preview"
  ],
  "python.analysis.downloadChannel": "daily",
  "python.analysis.openFilesOnly": false,
  "python.analysis.symbolsHierarchyDepthLimit": 12,
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.showAdvancedMembers": true,
  "python.autoUpdateLanguageServer": true,
  "python.linting.flake8Path": "python -m flake8",
  "python.linting.pylintEnabled": false,
  "python.venvFolders": ["envs", ".pyenv", ".env", ".venv", "~/.local/share/virtualenvs/*", ".direnv"],
  "python.terminal.executeInFileDir": true,
  "python.workspaceSymbols.exclusionPatterns": [
    "**/site-packages/**",
    "node_modules",
    "build",
    "dist",
    ".tox",
    ".venv"
  ],
  "refactor.openCommand": "vsplit",
  "signature.hideOnTextChange": true,
  "signature.maxWindowHeight": 14,
  "signature.preferShownAbove": false,
  "signature.triggerSignatureWait": 300,
  "snippets.extends": {
    "python": [
      "rst"
    ]
  },
  "snippets.shortcut": "CocSnip",
  "snippets.snipmate.author": "Faris A. Chugthai",
  "snippets.snipmate.enable": false,
  "snippets.ultisnips.directories": [
    "~/.config/nvim/UltiSnips"
  ],
  "suggest.completionItemKindLabels": {
    "class": "",
    "color": "",
    "constant": "",
    "default": "",
    "enum": "Ψ",
    "enumMember": "",
    "event": "鬒",
    "field": "料",
    "file": "",
    "folder": "",
    "function": "ƒ",
    "interface": "",
    "keyword": "",
    "method": "",
    "module": "",
    "operator": "",
    "property": "",
    "reference": "渚",
    "snippet": "",
    "struct": "פּ",
    "text": "",
    "typeParameter": "",
    "unit": "",
    "value": "",
    "variable": ""
  },
  "suggest.enablePreview": true,
  "suggest.fixInsertedWord": true,
  "suggest.floatEnable": true,
  "suggest.keepCompleteopt": true,
  "suggest.maxCompleteItemCount": 100,
  "suggest.noselect": true,
  "suggest.removeDuplicateItems": true,
  "tsserver.npm": "yarn",
  "typescript.showUnused": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeFunctionCalls": true
}
