    "--number-select",
    "--no-sort",
    "--top"
  ],
  "list.source.quickfix.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.searchhistory.defaultOptions": [
    "--top",
    "--tab",
    "--regex"
  ],
  "list.source.services.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.sessions.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.snippets.defaultOptions": [
    "--auto-preview",
    "--top",
    "--ignore-case"
  ],
  "list.source.sources.defaultArgs": [
    "-refresh"
  ],
  "list.source.sources.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.symbols.defaultOptions": [
    "--top",
    "--normal",
    "--interactive",
    "--number-select"
  ],
  "list.source.tags.defaultOptions": [
    "--top",
    "--normal"
  ],
  "list.source.words.defaultArgs": [
    "-word"
  ],
  "list.source.words.defaultOptions": [
    "--top",
    "--interactive",
    "--normal",
    "--auto-preview"
  ],
  "list.source.yank.defaultArgs": [
    "preview"
  ],
  "list.source.yank.defaultOptions": [
    "--top",
    "--tab",
    "--ignore-case",
    "--regex",
    "--normal",
    "--auto-preview"
  ],
  "python.analysis.diagnosticEnabled": true,
  "python.analysis.diagnosticPublishDelay": 300,
  "python.analysis.downloadChannel": "daily",
  "python.analysis.logLevel": "Information",
  "python.analysis.openFilesOnly": false,
  "python.analysis.symbolsHierarchyDepthLimit": 12,
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.showAdvancedMembers": true,
  "python.autoUpdateLanguageServer": true,
  "python.linting.flake8Path": "python -m flake8",
  "python.linting.pylintEnabled": false,
  "python.pipenvPath": "pipenv",
  "python.pythonPath": "python3",
  "python.terminal.executeInFileDir": true,
